var assert = new Assert();

var data = "tit1   tit2 tit3       tit4\n"
	 + "data1 data2 data3 data4\n"
	 + "data1 data2 data3\n"
	 + "data1 data2 data3 data4 data5\n"
	 + "\n"
	 + "data1 data2 data3 data4\n"

